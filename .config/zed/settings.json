{
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Dark"
  },

  "ui_font_family": "Zed Plex Sans",
  "ui_font_size": 15,

  "buffer_font_family": "Monaco",
  "buffer_font_size": 14,

  "buffer_line_height": {
    "custom": 1.4
  },


  "base_keymap": "SublimeText",
  "multi_cursor_modifier": "cmd_or_ctrl",


  "current_line_highlight": "gutter",

  "show_whitespaces": "selection",

  "wrap_guides": [100],

  "soft_wrap": "none",
  "preferred_line_length": 120,


  "when_closing_with_no_tabs": "close_window",

  "seed_search_query_from_cursor": "selection",
  "use_smartcase_search": true,

  "double_click_in_multibuffer": "open",

  "pane_split_direction_horizontal": "down",
  "pane_split_direction_vertical": "right",

  "vertical_scroll_margin": 1,


  "enable_language_server": true,

  "hover_popover_enabled": true,

  "auto_signature_help": false,
  "show_signature_help_after_edits": false,

  "show_completions_on_input": true,
  "show_completion_documentation": true,

  "show_inline_completions": false,

  "features": {
    "inline_completion_provider": "none"
  },

  "inlay_hints": {
    "enabled": true
  },

  "diagnostics": {
    "include_warnings": true
  },


  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "cargo": {
          "autoreload": true,

          "allTargets": false,
          "features": [],
          "noDefaultFeatures": false,

          "cfgs": {},
          "target": null,

          "extraArgs": [],
          "extraEnv": {},

          "targetDir": null
        },

        "checkOnSave": true,

        "check": {
          "command": "check",

          "workspace": true,
          "allTargets": false,
          "features": null,
          "noDefaultFeatures": null,

          "targets": null,

          "extraArgs": [],
          "extraEnv": {},

          "ignore": []
        },

        "diagnostics": {
          "enable": true,

          "experimental": { "enable": false },
          "styleLints": { "enable": true },

          "warningsAsInfo": [],
          "warningsAsHint": [],
          "disabled": []
        },

        "workspace": {
          "symbol": {
            "search": {
              "scope": "workspace",
              "kind": "all_symbols",
              "limit": 128
            }
          }
        },

        "references": {
          "excludeImports": true,
          "excludeTests": true
        },

        "signatureInfo": {
          "detail": "parameters",
          "documentation": { "enable": false }
        },

        "completion": {
          "fullFunctionSignatures": { "enable": false },

          "callable": {
            "snippets": "fill_arguments"
          },

          "autoimport": { "enable": true },
          "privateEditable": { "enable": true },
          "postfix": { "enable": true },

          "autoself": { "enable": false },
          "termSearch": { "enable": false }
        },

        "imports": {
          "group": { "enable": true },

          "granularity": {
            "group": "crate",
            "enforce": false
          },

          "merge": {
            "glob": true
          },

          "prefix": "self",

          "preferPrelude": false,
          "preferNoStd": false
        },

        "assist": {
          "expressionFillDefault": "todo",
          "emitMustUse": false
        },

        "semanticHighlighting": {
          "strings": { "enable": true },

          "operator": {
            "enable": true,
            "specialization": { "enable": false }
          },

          "punctuation": {
            "enable": false,
            "specialization": { "enable": false },

            "punctuation": {
              "separate": {
                "macro": {
                  "bang": false
                }
              }
            }
          },

          "doc": {
            "comment": {
              "inject": { "enable": true }
            }
          },

          "nonStandardTokens": true
        },

        "inlayHints": {
          "maxLength": 25,

          "renderColons": true,
          "closureStyle": "rust_analyzer",

          "typeHints": {
            "enable": true,
            "hideNamedConstructor": true,
            "hideClosureInitialization": true
          },

          "closureReturnTypeHints": { "enable": "with_block" },
          "closureCaptureHints": { "enable": false },

          "parameterHints": { "enable": true },
          "chainingHints": { "enable": true },

          "bindingModeHints": { "enable": false },

          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },

          "reborrowHints": { "enable": "never" },
          "implicitDrops": { "enable": false },

          "expressionAdjustmentHints": {
            "enable": "never",
            "hideOutsideUnsafe": true,
            "mode": "prefix"
          },

          "discriminantHints": { "enable": "never" },
          "rangeExclusiveHints": { "enable": false },

          "closingBraceHints": {
            "enable": false,
            "minLines": 25
          }
        },

        "highlightRelated": {
          "references": { "enable": true },

          "closureCaptures": { "enable": true },

          "exitPoints": { "enable": true },
          "yieldPoints": { "enable": true },
          "breakPoints": { "enable": true }
        },

        "lens": {
          "enable": true,

          "location": "above_name",

          "run": { "enable": true },
          "debug": { "enable": false },

          "references": {
            "adt": { "enable": false },
            "enumVariant": { "enable": false },
            "method": { "enable": false },
            "trait": { "enable": false }
          },

          "implementations": { "enable": false }
        },

        "hover": {
          "documentation": {
            "enable": true,
            "keywords": { "enable": true }
          },

          "links.enable": true,

          "show": {
            "fields": 5,
            "enumVariants": 5,
            "traitAssocItems": null
          },

          "memoryLayout": {
            "enable": false,
            "size": "decimal",
            "alignment": "decimal",
            "offset": "decimal",
            "niches": false
          },

          "actions": {
            "enable": false,

            "run": { "enable": false },
            "debug": { "enable": false },

            "gotoTypeDef": { "enable": false },
            "references": { "enable": false },

            "implementations": { "enable": false }
          }
        },

        "rustfmt": {
          "rangeFormatting.enable": true,
          "extraArgs": ["+nightly"]
        },

        "joinLines": {
          "joinAssignments": true,
          "joinElseIf": true,
          "unwrapTrivialBlock": true,
          "removeTrailingComma": true
        },

        "procMacro": {
          "enable": true,
          "attributes.enable": true,
          "ignored": {}
        },

        "interpret": {
          "tests": false
        },

        "notifications": {
          "cargoTomlNotFound": true,
        },

        "cachePriming": {
          "enable": true,
          "numThreads": 0
        }
      }
    }
  },


  "formatter": "auto",

  "format_on_save": "off",
  "use_on_type_format": false,


  "assistant": {
    "enabled": true,

    "button": true,
    "dock": "right",

    "version": "2",

    "default_model": {
      "provider": "ollama",
      "model": "codellama:latest"
    },

    "default_open_ai_model": "gpt-4o"
  },


  "jupyter": {
    "enabled": false
  },

  "journal": {
    "path": "~",
    "hour_format": "hour24"
  },


  "file_types": {
    "JSONC": ["sublime-settings", "sublime-keymap", "sublime-mousemap"]
  },


  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false
  },

  "preview_tabs": {
    "enabled": true,
    "enable_preview_from_file_finder": true,
    "enable_preview_from_code_navigation": true
  },

  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false
  },

  "gutter": {
    "line_numbers": true,
    "folds": true,
    "code_actions": false,
    "runnables": false
  },

  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": false
    }
  },

  "project_panel": {
    "button": true,
    "dock": "left",

    "auto_reveal_entries": false,

    "default_width": 300,
    "indent_size": 15
  },

  "outline_panel": {
    "button": true,
    "dock": "left",

    "auto_reveal_entries": false,

    "default_width": 300,
    "indent_size": 15
  },

  "notification_panel": {
    "button": false,
    "dock": "right"
  },

  "collaboration_panel": {
    "button": false,
    "dock": "right"
  },

  "chat_panel": {
    "button": false,
    "dock": "right"
  },


  "show_call_status_icon": true,

  "calls": {
    "mute_on_join": true,
    "share_on_join": false
  },


  "auto_update": true,

  "auto_install_extensions": {
    "html": false
  },


  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
