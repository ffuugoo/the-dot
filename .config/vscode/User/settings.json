{
    // VS Code

    // Color Scheme & Font
    "workbench.colorTheme": "Default Light Modern",

    "editor.fontFamily": "Monaco",
    "editor.fontSize": 14,

    "editor.letterSpacing": -0.1, // ü§î
    "editor.lineHeight": 20, // ü§î

    "workbench.fontAliasing": "auto",

    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.activeIconPack": "none",

    "editor.tokenColorCustomizations": {
        "keywords": "#c695c6",

        "functions": "#6699cc",
        "types": "#c695c6",

        "variables": "#333333",

        "numbers": "#f9ae58",
        "strings": "#80b979",

        "comments": "#999999",

        "textMateRules": [
            {
                "name": "Keyword",
                "scope": ["keyword", "keyword.control", "keyword.other"],
                "settings": {
                    "foreground": "#c695c6",
                }
            },
            {
                "name": "Type Declaration Keyword",
                "scope": ["storage", "storage.type", "storage.modifier"],
                "settings": {
                    "foreground": "#c695c6",
                }
            },
            {
                "name": "Type Declaration Name",
                "scope": ["entity.name.type.struct", "entity.name.type.enum"],
                "settings": {
                    "foreground": "#f9ae58",
                }
            },
            {
                "name": "Enum Variant Name",
                "scope": "variable.other.enummember", // semantic
                "settings": {
                    "foreground": "#c695c6",
                }
            },
            {
                "name": "Lifetime",
                "scope": [
                    "storage.modifier.lifetime", // semantic
                    "punctuation.definition.lifetime",
                    "entity.name.type.lifetime",
                ],
                "settings": {
                    "foreground": "#ec5f66",
                }
            },
            {
                "name": "Function Declaration",
                "scope": "meta.function.definition entity.name.function",
                "settings": {
                    "foreground": "#5fb4b4",
                }
            },
            {
                "name": "Function Parameters",
                "scope": "meta.function.definition variable.other",
                "settings": {
                    "foreground": "#f9ae58",
                }
            },
            {
                "name": "Constant Declaration",
                "scope": [
                    "variable.other.constant", // semantic
                    "constant.other.caps",
                ],
                "settings": {
                    "foreground": "#f9ae58",
                }
            },
            {
                "name": "Macro Call",
                "scope": "entity.name.function.preprocessor", // semantic
                "settings": {
                    "foreground": "#6699cc",
                }
            },
            {
                "name": "Module Declaration",
                "scope": "entity.name.module",
                "settings": {
                    "foreground": "#f9ae58",
                }
            },
            {
                "name": "Namespace",
                "scope": "entity.name.namespace",
                "settings": {
                    "foreground": "#333333",
                }
            },
            {
                "name": "Attribute",
                "scope": "meta.attribute entity.name.function",
                "settings": {
                    "foreground": "#6699cc",
                }
            },
            {
                "name": "Derive Macro",
                "scope": "meta.attribute entity.name.type",
                "settings": {
                    "foreground": "#333333",
                }
            },

            {
                "name": "JSON Property Name",
                "scope": "support.type.property-name.json",
                "settings": {
                    "foreground": "#c695c6",
                }
            },

            {
                "name": "Language Constant",
                "scope": "constant.language",
                "settings": {
                    "foreground": "#ec5f66",
                }
            },
        ],
    },

    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "derive": "#333333",

            "struct.declaration": "#f9ae58",
            "enum.declaration": "#f9ae58",

            "function.declaration": "#5fb4b4",
            "parameter.declaration": "#f9ae58",

            "struct": "#c695c6",
            "enum": "#c695c6",
        }
